{
  "name": "website",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@hookform/resolvers": "^2.9.11",
    "@reduxjs/toolkit": "^1.9.3",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "jquery": "^3.6.4",
    "react": "^18.2.0",
    "react-axios": "^2.0.6",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.43.5",
    "react-redux": "^8.0.5",
    "react-router-dom": "^6.8.2",
    "react-switch": "^7.0.0",
    "web-vitals": "^2.1.4",
    "yup": "^1.0.2"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "jest",
    "coverage": "jest --coverage ",
    "eject": "react-scripts eject",
    "mutation": "stryker run",
    "lol": "node -e \"const val =  parseInt(%npm_config_coveragethreshold%); console.log(val)\""
  },
  "jest": {
    "collectCoverageFrom": [
      "src/pages/**/*.js",
      "src/*.js"
    ],
    "coverageThreshold": {
      "global": {
        "branches": "node -e \"const val =  parseInt(%npm_config_coveragethreshold%); console.log(val)\"",
        "functions": 70,
        "lines": 70,
        "statements": 70
      }
    }
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@stryker-mutator/jest-runner": "^6.4.1",
    "jest": "^27.5.1",
    "react-scripts": "^5.0.1"
  }
}
